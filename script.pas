//Persegi panjang
program Persegi_Panjang;
var
    panjang : integer;
    lebar   : integer;
    luas    : integer;

begin
    write ('Masukkan panjang =');read(panjang);
    write ('Masukan Lebar ='); read(lebar);
    luas:=panjang*lebar;
    writeln('Luas Persegi panjang =',luas);
end.

